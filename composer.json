{
    "name": "duncan3dc/github",
    "description": "A library to interact with the GitHub API",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.4",
        "lcobucci/jwt": "^3.2.2",
        "psr/http-message": "^1.0",
        "ext-json": "*",
        "php": "^7.1"
    },
    "require-dev": {
        "duncan3dc/object-intruder": "^0.3",
        "maglnet/composer-require-checker": "^1.0",
        "mockery/mockery": "^1.1",
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-mockery": "^0.10.2",
        "phpstan/phpstan-phpunit": "^0.10.0",
        "squizlabs/php_codesniffer": "^3.3",
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {"duncan3dc\\GitHub\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"duncan3dc\\GitHubTests\\": "tests/"}
    }
}
